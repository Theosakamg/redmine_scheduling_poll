<h2>SchedulingPolls#edit</h2>

<div id="scheduling-poll">
  <h3><%= link_to @poll.issue.subject, issue_path(@poll.issue) %></h3>
  <% action = (@poll.persisted?)? "update" : "create" %>
  <%= form_for @poll, :url => url_for(:controller => "scheduling_polls", :action => action) do |f| %>
    <%= render :partial => 'form', :locals => { :f => f} %>
  <% end %>
</div>

